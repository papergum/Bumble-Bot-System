{
  "api": {
    "host": "0.0.0.0",
    "port": 8000,
    "debug": false,
    "reload": true,
    "workers": 4,
    "cors_origins": ["*"],
    "api_prefix": "/api/v1"
  },
  "security": {
    "secret_key": "REPLACE_WITH_SECURE_SECRET_KEY",
    "algorithm": "HS256",
    "access_token_expire_minutes": 60
  },
  "rate_limiting": {
    "enabled": true,
    "requests_per_minute": 60
  },
  "logging": {
    "level": "INFO",
    "format": "%(asctime)s - %(name)s - %(levelname)s - %(message)s"
  }
}